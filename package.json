{
  "name": "go-buildinfo",
  "version": "1.0.0",
  "private": true,
  "description": "GitHub Action to populate buildinfo for Go.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daaku/go-buildinfo"
  },
  "author": "Naitik Shah",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.4.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.2",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.28.0",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^27.0.4",
    "jest-circus": "^27.0.4",
    "js-yaml": "^4.1.0",
    "prettier": "^2.3.1",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.2"
  }
}
